{
    "name": "tenolo/doctrine-table-prefix-bundle",
    "description": "Adds the feature to add table prefix to an entity.",
    "keywords": [
        "doctrine",
        "table",
        "prefix",
        "entity",
        "symfony",
        "bundle",
        "tenolo"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0|~4.0",
        "symfony/http-kernel": "~2.8|~3.0|~4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/doctrine-bridge": "~2.8|~3.0|~4.0",

        "doctrine/orm": "~2.5",
        "doctrine/common": "~2.6",
        "doctrine/doctrine-bundle": "~1.6",

        "tenolo/utilities": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\DoctrineTablePrefixBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Bundle\\DoctrineTablePrefixBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
